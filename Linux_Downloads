##Run this code for downloads and updates python, github, etc.
sudo apt-get update
sudo dpkg -i Downloads/google-chrome-stable_current_amd64.deb 


##set up git
sudo apt-get install git
git config --global user.name "Suzi O'Rourke"
git config --global user.email "wellersuzi@gmail.com"
ssh-keygen -t ed25519 -C "wellersuzi@gmail.com"
ssh-add ~/.ssh/id_ed25519
cat ~/.ssh/id_ed25519.pub
## The output of the above will get copied into https://github.com/settings/keys

mkdir Development
cd Development
git clone git@github.com:Orourkes/NotesforLinuxSetup.git

cd

sudo apt install build-essential 
sudo apt-get install iproute2 gawk python3 gparted \
build-essential gcc git make net-tools libncurses5-dev \
tftpd zlib1g-dev libssl-dev flex bison libselinux1 gnupg \
wget git-core diffstat chrpath socat xterm autoconf libtool\
tar unzip texinfo zlib1g-dev gcc-multilib automake \
zlib1g:i386 screen pax gzip cpio python3-pip python3-pexpect \
xz-utils debianutils iputils-ping python3-git python3-jinja2 
libegl1-mesa libsdl1.2-dev pylint3 net-tools gedit geany vim
