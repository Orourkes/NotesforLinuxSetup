##Run this code for downloads and updates python, github, etc.
sudo apt-get update
sudo dpkg -i Downloads/google-chrome-stable_current_amd64.deb 


##set up git
sudo apt-get install git
git config --global user.name "Suzi O'Rourke"
git config --global user.email "wellersuzi@gmail.com"
ssh-keygen -t ed25519 -C "wellersuzi@gmail.com"
ssh-add ~/.ssh/id_ed25519
cat ~/.ssh/id_ed25519.pub
## The output of the above will get copied into https://github.com/settings/keys

mkdir Development
cd Development
git clone git@github.com:Orourkes/NotesforLinuxSetup.git

